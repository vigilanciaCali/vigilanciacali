<?xml version="1.0" encoding="UTF-8"?>
<Configuration monitorInterval="60">

	<Appenders>
		<Console name="Console-Appender" target="SYSTEM_OUT">
			<PatternLayout>
				<pattern>
					%-4r [%p] %-c %d{ISO8601}  %m%n
				</pattern>
			</PatternLayout>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console-Appender" />
		</Root>
	</Loggers>
	
</Configuration>